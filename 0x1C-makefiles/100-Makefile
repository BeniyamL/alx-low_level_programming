#simple make files using varibles and object files, and many rules

CC = gcc
SRC = main.c holberton.c
OBJ = $(SRC:.c=.o)
NAME = holberton
CFLAGS = -Wall -Werror -Wextra -pedantic
all : m.h $(OBJ)
	$(CC)  $(OBJ) -o $(NAME)
clean:
	$(RM) -f *~ $(NAME)
oclean:
	$(RM) -f $(OBJ)
fclean: clean oclean
re: fclean all
